<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.tour.mapper.ServiceMapper">
	<select id="selectAccomList" resultType="com.spring.tour.vo.Accom_serviceVo" parameterType="string">
		select * from accom_service where user_id=#{user_id}
	</select>
	<select id="selectTourList" resultType="com.spring.tour.vo.TourServiceVo" parameterType="string">
		select * from tour_service where user_id=#{user_id}
	</select>
	
</mapper>